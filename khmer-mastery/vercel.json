{
  "git": {
    "deploymentEnabled": true
  },
  "headers": [
    {
      "source": "/vendor/(.*).wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        },
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/vendor/:path*",
      "destination": "/vendor/:path*"
    },
    {
      "source": "/((?!vendor/|fonts/|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}